html
	head
		title Deliver to?
		link(rel='stylesheet', href='https://fonts.googleapis.com/icon?family=Material+Icons')
		link(rel='stylesheet', href='https://code.getmdl.io/1.3.0/material.indigo-pink.min.css')
		script(defer='', src='https://code.getmdl.io/1.3.0/material.min.js')
		link(rel='stylesheet', type='text/css', href='/static/styles/style.css')
	body
		nav
			img(src="/static/images/logo.png")
			h1 Where to?
		table#DeliverTable.mdl-data-table.mdl-js-data-table.mdl-data-table--selectable.mdl-shadow--2dp
			tbody
				tr
					td.mdl-data-table__cell--non-numeric School of IT
					td
						label.mdl-radio.mdl-js-radio.mdl-js-ripple-effect(for="SIT")
							input#SIT.mdl-radio__button(type="radio", name="options", value="1")
				tr
					td.mdl-data-table__cell--non-numeric ABS Building
					td
						label.mdl-radio.mdl-js-radio.mdl-js-ripple-effect(for="ABS")
							input#ABS.mdl-radio__button(type="radio", name="options", value="1")
				tr
					td.mdl-data-table__cell--non-numeric Carslaw
					td
						label.mdl-radio.mdl-js-radio.mdl-js-ripple-effect(for="Cars")
							input#Cars.mdl-radio__button(type="radio", name="options", value="1")
				tr
					td.mdl-data-table__cell--non-numeric Peter Nicol Russell
					td
						label.mdl-radio.mdl-js-radio.mdl-js-ripple-effect(for="PNR")
							input#PNR.mdl-radio__button(type="radio", name="options", value="1")
				tr
					td.mdl-data-table__cell--non-numeric Wentworth
					td
						label.mdl-radio.mdl-js-radio.mdl-js-ripple-effect(for="Went")
							input#Went.mdl-radio__button(type="radio", name="options", value="1")
				tr
					td.mdl-data-table__cell--non-numeric Fisher Library
					td
						label.mdl-radio.mdl-js-radio.mdl-js-ripple-effect(for="Fish")
							input#Fish.mdl-radio__button(type="radio", name="options", value="1")
				tr
					td.submit-row(colspan=2): button.mdl-button.mdl-js-button.mdl-js-ripple-effect.submit-btn(onclick="location.href = 'http://localhost:5000/accepted';") PAY

		

